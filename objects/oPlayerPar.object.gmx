var vxNew, vyNew;

cx += vx;
cy += vy;
vxNew = round(cx);
vyNew = round(cy);
cx -= vxNew;
cy -= vyNew;



repeat (abs(vyNew)) {
    y += sign(vy);
}

repeat (abs(vxNew)) {
    x += sign(vx);
}


