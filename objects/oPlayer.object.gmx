            <string>///Collisions


//Slope
    if (place_meeting(x+sign(vx), y, oBlock) &amp;&amp;
        !place_meeting(x+sign(vx), y-1, oBlock)){
        
        repeat(abs(vx)){        //Sigue sin fucnionar bien
            y--;
        }  
    }


//Corner collision
var vxNew, vyNew;
vxNew=abs(vx);
vyNew=abs(vy);

if(place_meeting(x+vx, y+vy, oBlock)){
    while(!place_meeting(x+sign(vx), y+sign(vy), oBlock)){
        if(vyNew&gt;vxNew){
            y+=sign(vy);
            vyNew--;
        }else{
            x+=sign(vx);
            vxNew--;
        }
    }
}

//Vertical collision
if(place_meeting(x, y+vy, oBlock)){
    while(!place_meeting(x, y+sign(vy), oBlock)){
        y+=sign(vy);
    }
    vy=0;
}

//Horizontal collision
if(place_meeting(x+vx, y, oBlock)){
    while(!place_meeting(x+sign(vx), y, oBlock)){
        x+=sign(vx);
    }
    vx=0;
}












