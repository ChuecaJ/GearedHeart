            <string>///Collisions

//Move along the platforms
//Horizontal
vx=0;
if(place_meeting(x, y+1, oPlatformH1)){
    platform=instance_place(x, y+1, oPlatformH1);
    if(platform.hspeed!=0){
        vx+=platform.hspeed;
    }
}
//Vertical [in the platform]

//Vertical collision
if(place_meeting(x, y+vy, oBlock)){
    yDifference = y - instance_place(x, y+vy, oBlock).y;
    while(!place_meeting(x, y+sign(vy), oBlock)){
        y-=sign(yDifference);
    }
    vy=0;
}

//Horizontal collision
if(place_meeting(x+vx, y, oBlock)){
    xDifference = x - instance_place(x+vx, y, oBlock).x;
    while(!place_meeting(x+sign(vx), y, oBlock)){
        x-=sign(xDifference);
    }
    vx=0;
}

//Handle platforms
if(place_meeting(x, y, oPlatformV1)){
    move_outside_solid(90, 10);
}
  

//Final, just in case
if(xDifference&lt;=0){
    move_outside_solid(180, 60);
}else{
    move_outside_solid(0, 60);    
}



</string>
