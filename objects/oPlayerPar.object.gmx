        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Collisions


//Corner collision
var vxNew, vyNew;
vxNew=abs(vx);
vyNew=abs(vy);



if(place_meeting(x+vx, y+vy, oBlock)){
    xDifference = x - instance_place(x+vx, y+vy, oBlock).x;
    yDifference = y - instance_place(x+vx, y+vy, oBlock).y;
    while(!place_meeting(x+sign(vx), y+sign(vy), oBlock)){
        if(vyNew&gt;vxNew){
            y-=sign(yDifference);
            vyNew--;
        }else{
            x-=sign(xDifference);
            vxNew--;
        }
    }
}

//Vertical collision
if(place_meeting(x, y+vy, oBlock)){
    while(!place_meeting(x, y+sign(vy), oBlock)){
        y-=sign(yDifference);
    }
    vy=0;
}

//Horizontal collision
if(place_meeting(x+vx, y, oBlock)){
    while(!place_meeting(x+sign(vx), y, oBlock)){
        x-=sign(xDifference);
    }
    vx=0;
}

//Final, just in case
//move_outside_solid(90, 60);









</string>
